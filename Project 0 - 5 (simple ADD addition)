#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int ADD(int x, int y)
{
	int z = 0;
	z = x + y;
	return z;
}
int main()
{
	int num1 = 0;
	int num2 = 0;
	printf("请输入加数1：");
	scanf("%d",&num1);
	printf("请输入加数2：");
	scanf("%d", &num2);
	int SUM = ADD(num1, num2);
	printf("上述加法计算结果为：%d\n", SUM);
	return 0;
}
